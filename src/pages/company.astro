   <Whatsapp client:load />
</Layout>

<!-- Animation Script Section -->
<script>
  // Section observer for "coming out" effect
  const sectionObserver = new IntersectionObserver((entries) => {
    entries.forEach(entry => {
      if (entry.isIntersecting) {
        entry.target.classList.add('section-revealed');
        sectionObserver.unobserve(entry.target);
      }
    });
  }, {
    threshold: 0.18,
    rootMargin: '0px 0px -80px 0px'
  });

  // Hero observer for banner cinematic effect
  const heroObserver = new IntersectionObserver((entries) => {
    entries.forEach(entry => {
      if (entry.isIntersecting) {
        entry.target.classList.add('hero-revealed');
        heroObserver.unobserve(entry.target);
      }
    });
  }, {
    threshold: 0.12,
    rootMargin: '0px 0px -40px 0px'
  });

  document.addEventListener('DOMContentLoaded', () => {
    // Hero banner
    const heroSection = document.querySelector('.company-hero');
    if (heroSection) heroObserver.observe(heroSection);

    // All main sections (except hero)
    document.querySelectorAll('.main-section').forEach(section => {
      sectionObserver.observe(section);
    });
  });
</script>

<style>
  /* Banner cinematic animation */
  .company-hero {
    opacity: 0;
    transform: translateY(60px);
    transition: all 1.6s cubic-bezier(0.25, 0.46, 0.45, 0.94);
  }
  .company-hero.hero-revealed {
    opacity: 1;
    transform: translateY(0);
  }
  .hero-image-reveal {
    transform: scale(1.08);
    opacity: 0;
    transition: all 2.2s cubic-bezier(0.25, 0.46, 0.45, 0.94);
  }
  .company-hero.hero-revealed .hero-image-reveal {
    transform: scale(1);
    opacity: 1;
    transition-delay: 0.2s;
  }
  .hero-overlay-fade {
    opacity: 0;
    transition: opacity 1.5s cubic-bezier(0.25, 0.46, 0.45, 0.94);
  }
  .company-hero.hero-revealed .hero-overlay-fade {
    opacity: 1;
    transition-delay: 0.5s;
  }
  .hero-content-reveal {
    opacity: 0;
    transform: translateY(40px);
    transition: all 1.3s cubic-bezier(0.25, 0.46, 0.45, 0.94);
  }
  .company-hero.hero-revealed .hero-content-reveal {
    opacity: 1;
    transform: translateY(0);
    transition-delay: 0.7s;
  }
