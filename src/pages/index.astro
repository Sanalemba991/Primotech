---
import Layout from "../layouts/Layout.astro";
import Banner from "../components/Banner.astro";
import Products from "../components/Products.astro";
import Cataegories from "../components/Cataegories.astro";
import WhyUs from "../components/WhyUs.astro";
import Question from "../components/Question.astro";
import Whatsapp from "../components/Whatsapp.tsx";

const title: string =
  "PrimoTech LLC - Authorized Uniarch Distributor in Dubai, UAE";
const description: string =
  "PrimoTech LLC is the official distributor of Uniarch products in Dubai, UAE, offering reliable surveillance solutions, IT services, and smart technology integration.";


const structuredData = {
  "@context": "https://schema.org",
  "@graph": [
    {
      "@type": "Organization",
      "@id": "https://promotechllc.com/#organization",
      name: "Primotech Uniarch Dubai",
      url: "https://promotechllc.com",
      logo: {
        "@type": "ImageObject",
        "@id": `${Astro.url.origin}/log.png`,
        url: `${Astro.url.origin}/log.png`,

        caption: "PrimoTech Uniarch Dubai",
        inLanguage: "en-US",
        width: 1000,
        height: 375,
      },
    },
    {
      "@type": "WebSite",
      "@id": "https://promotechllc.com/#website",
      url: "https://promotechllc.com",
      name: "PrimoTech | Uniarch Authorized Distributor in UAE ",
      publisher: {
        "@id": "https://promotechllc.com/#organization",
      },
      inLanguage: "en-US",
      potentialAction: {
        "@type": "SearchAction",
        target: "https://promotechllc/?s={search_term_string}",
        "query-input": "required name=search_term_string",
      },
    },
    {
      "@type": "ImageObject",
      "@id": `${Astro.url.origin}/log.png`,
      url: `${Astro.url.origin}/log.png`,
      width: 200,
      height: 200,
      inLanguage: "en-US",
    },
    {
      "@type": "WebPage",
      "@id": "https://promotechllc.com/#webpage",
      url: "https://promotechllc.com/",
      name: "PrimoTech Uniarch Authorized Distributor in Dubai, UAE | Security Solutions ",
      description: description,
      datePublished: "2023-05-05T12:19:39+01:00",
      dateModified: "2024-11-25T11:05:33+00:00",
      about: {
        "@id": "https://promotechllc.com/#organization",
      },
      isPartOf: {
        "@id": "https://promotechllc.com/#website",
      },
      primaryImageOfPage: {
        "@id": `${Astro.url.origin}/log.png`,
      },
      inLanguage: "en-US",
    },
    {
      "@type": "Article",
      headline:
        "PrimoTech Uniarch Security Solutions in Dubai - Authorized Distributor",
      datePublished: "2023-05-05T12:19:39+01:00",
      dateModified: "2024-11-25T11:05:33+00:00",
      publisher: {
        "@id": "https://promotechllc.com/#organization",
      },
      description: description,
      name: title,
      "@id": "https://promotechllc.com/#richSnippet",
      isPartOf: {
        "@id": "https://promotechllc.com/#webpage",
      },
      inLanguage: "en-US",
      mainEntityOfPage: {
        "@id": "https://promotechllc.com/#webpage",
      },
      author: {
        "@type": "Organization",
        name: "UNV Dubai",
      },
      image: {
        "@type": "ImageObject",
        url: `${Astro.url.origin}/log.png`,
        width: 1200,
        height: 630,
        caption: "PrimoTech Uniarch Dubai Authorized Distributor",
      },
      speakable: {
        "@type": "SpeakableSpecification",
        cssSelector: [".hero", ".testimonials"],
      },
    },
    {
      "@type": "LocalBusiness",
      "@id": "https://promotechllc.com/#localbusiness",
      name: "Primotech Uniarch Dubai",
      image: `${Astro.url.origin}/log.png`,
      address: {
        "@type": "PostalAddress",
        streetAddress: "Your Street Address",
        addressLocality: "Dubai",
        addressRegion: "Dubai",
        postalCode: "Your Postal Code",
        addressCountry: "AE",
      },
      geo: {
        "@type": "GeoCoordinates",
        latitude: "25.2048", // Replace with actual coordinates
        longitude: "55.2708",
      },
      telephone: "+971 52 879 6664",
      priceRange: "$$",
      openingHoursSpecification: [
        {
          "@type": "OpeningHoursSpecification",
          dayOfWeek: [
            "Monday",
            "Tuesday",
            "Wednesday",
            "Thursday",
            "Friday",
            "Saturday"
          ],
          opens: "09:00",
          closes: "18:00",
        },
      ],
    },
    {
      "@type": "BreadcrumbList",
      "@id": "https://promotechllc.com/#breadcrumb",
      itemListElement: [
        {
          "@type": "ListItem",
          position: 1,
          name: "Home",
          item: "https://promotechllc.com/",
        },
      ],
    },
  ],
};


---

<Layout
  title={title}
  description={description}
  structuredData={structuredData}
  canonicalUrl={Astro.url.href}
>
  <Banner />
  <Products />
  <Cataegories />
  <WhyUs />
  <Question />
  <Whatsapp client:load />
</Layout>
